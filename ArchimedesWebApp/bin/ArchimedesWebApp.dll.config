  http://go.microsoft.com/fwlink/?LinkId=169433
  -->

<configuration>
    <connectionStrings>
        <add name="SEI_ArchimedesConnectionString" connectionString="Data Source=csdb;Initial Catalog=SEI_Archimedes;Integrated Security=True"
            providerName="System.Data.SqlClient" />
        <add name="SEI_TimeMachine2ConnectionString" connectionString="Data Source=csdb;Initial Catalog=SEI_TimeMachine2;Integrated Security=True"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
  
    <appSettings>
       <add key="EnableSimpleMembership" value="false" />
    </appSettings>
  
    <system.web>
      <customErrors mode="Off"/>
      <compilation debug="true" targetFramework="4.0" />

      <authentication mode="Windows" />
    </system.web>

</configuration>
